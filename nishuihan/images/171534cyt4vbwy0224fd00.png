nie.define('TOPBAR',function(){
        var barlist = {
            'topbar' : [function(_template_object
                                 /**/) {
                var _template_fun_array=[];
                var fn=(function(__data__){
                    var _template_varName='';
                    for(var name in __data__){
                        _template_varName+=('var '+name+'=__data__["'+name+'"];');
                    };
                    eval(_template_varName);
                    _template_fun_array.push(''); if (ranks.length > 0) { _template_fun_array.push('    '); for(var a =0,len = ranks.length;a< len;a++){_template_fun_array.push('        <li><a target="_blank" href="',typeof( ranks[a][0]) === 'undefined'?'':baidu.template._encodeHTML( ranks[a][0]),'">',typeof( ranks[a][1]) === 'undefined'?'':baidu.template._encodeHTML( ranks[a][1]),'</a><em class="',typeof( ranks[a][2]) === 'undefined'?'':baidu.template._encodeHTML( ranks[a][2]),'"></em></li>    '); } _template_fun_array.push(''); } else{ _template_fun_array.push('    '); } _template_fun_array.push('');
                    _template_varName=null;
                })(_template_object);
                fn = null;
                return _template_fun_array.join('');

            }][0]
        };
        var init = function(){
                    $('.nav-li-box1 ul').html(getTopbarCon($('#NIE-topBar-menu .NIE-list li:lt(2) a')));
                    $('.nav-li-box2 ul').html(getTopbarCon($('#NIE-topBar-menu .NIE-list li:eq(2) a[class!="NIE-topBar-more"]')));
                    $('.nav-li-box3 ul').html(getTopbarCon($('#NIE-topBar-menu .NIE-list .hot a')));
                    $('.nav-li-box4 ul').html(getTopbarCon($('#NIE-list-news a')));
                    // getTopbarCon($('#NIE-topBar-menu .NIE-list li:first a'));
                },
                getTopbarCon =  function($obj){
                    var gameurl = [],gamename = [],gameclass = [],roleplay = [],temp;
                    $obj.each(function(ind,ele){
                        var $this = $(this),
                                _gameclass = '';

                        gameurl[ind] = $this.attr('href');
                        if($this.children('em').length == 0){
                            gamename[ind] = $this.html();
                            gameclass[ind] = '';
                        }else{
                            gamename[ind] = $this.children('em').html();
                            _gameclass = $this.children('em').attr('class');
                            gameclass[ind] = _gameclass == 'NIE-topBar-new' ? 'icon-N' : 'icon-H';

                        }
                        roleplay[ind] = [gameurl[ind],gamename[ind],gameclass[ind]];
                    })
                    for(var i =0,ilen = roleplay.length - 1;i<ilen;i++){
                        for(var j = 0,jlen = roleplay.length - i -1;j<jlen;j++){
                            if(roleplay[j][2] < roleplay[j+1][2] && roleplay[j][2] != 'icon-H'){
                                temp = roleplay[j];
                                roleplay[j] = roleplay[j+1];
                                roleplay[j+1] = temp;
                            }
                        }
                    }
                    // console.log(roleplay);

                    var gamedata = barlist['topbar']({
                        ranks : roleplay
                    })
                    return gamedata;

                }
        init();
    })
</script>
