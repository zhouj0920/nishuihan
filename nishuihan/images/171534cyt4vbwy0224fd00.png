                fat: "#share", //分享组件插入父级（dom或选择器），默认："#NIE-share"
                type: 1, //分享组件类别，默认:1（1：小icon，2：大icon，3：右侧滚动条分享，4：图片分享，5：文本分享）        
                title: shareTxt.title, //分享的标题,默认：当前页面的title
                url: shareTxt.url, //分享的链接，默认：当前页面的url
